@model List<InmobiliariaWeb.Result.Programa.ReporteProgramasEstado>
@{
}

<div class="container">
    <div class="table-responsive-md">
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>N°</th>
                    <th>Programa</th>
                    <th>Manzana</th>
                    <th>Lt</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var Lotes in Model)
                {
                    <tr>
                        <td>@Lotes.Indice</td>
                        <td>@Lotes.Programa</td>
                        <td>@Lotes.Manzana</td>
                        <td>@Lotes.Lote</td>
                        <td class="g-2">
                            <a class="btn btn-outline-secondary" asp-controller="Programa" asp-action="Lado" asp-route-Ident010TipoLote="@Lotes.Ident010TipoLote"
                               asp-route-IdentLote="@Lotes.Ident_Lote">
                                <i class="bi bi-eye"></i>
                            </a>
                            <a class="btn btn-outline-secondary" href="#">
                                <i class="bi bi-coin"></i>
                            </a>
                        </td>

                    </tr>
                }
            </tbody>
        </table>
    </div>
    <div>
        <a class="btn btn-outline-info" asp-action="Index" asp-controller="Programa">REGRESAR</a>
    </div>
</div>